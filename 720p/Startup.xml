<window>
	<defaultcontrol always="true">1</defaultcontrol>
	<controls>

	<control type="group" id="1">
		<control type="button" id="2">
			<description>Set Defaults Button</description>
			<include>Objects_SetDefaults</include>
			<onfocus>SetFocus(3)</onfocus>
			<texturefocus>-</texturefocus>
			<texturenofocus>-</texturenofocus>
			<visible>!Skin.HasSetting(defaultsset)</visible>
		</control>
		<control type="button" id="3">
			<description>ReplaceWindow Trigger</description>
			<posx>35</posx>
			<posy>235</posy>
			<font>special15</font>
			<onfocus>ReplaceWindow(Home)</onfocus>
			<texturefocus>-</texturefocus>
			<texturenofocus>-</texturenofocus>
			<animation effect="fade" time="1000">Focus</animation>
		</control>
	</control>

	</controls>
</window>